<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

  <profiles>
    <profile>
      <id>overture-test-buildserver</id>
      <repositories>
        <repository>
          <id>overture-m2</id>
          <url>http://overture.au.dk/maven/symphony-overture-test/</url>
          <layout>default</layout>
        </repository>
        <repository>
          <id>overture-p2</id>
          <url>http://overture.au.dk/p2/symphony-overture-test/</url>
          <layout>p2</layout>
        </repository>
      </repositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>overture-test-buildserver</activeProfile>
  </activeProfiles>
</settings>
