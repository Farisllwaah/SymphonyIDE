<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="cml"
            id="eu.compassresearch.ide.cml.core.content-type"
            name="CML Content type"
            priority="high">
      </content-type>
   </extension>
   <extension
         id="eu.compassresearch.ide.cml.core.nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="eu.compassresearch.ide.cml.core.CmlProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.overture.ide.core.language">
      <Language
            dialect="VDM_CML"
            name="CML"
            nature="eu.compassresearch.ide.cml.core.nature">
         <ContentType
               id="eu.compassresearch.ide.cml.core.content-type">
         </ContentType>
      </Language>
   </extension>


</plugin>


