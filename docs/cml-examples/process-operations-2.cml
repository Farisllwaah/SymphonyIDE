process RegisterProc = 
 begin

  operatioNs  
   LOAD(i : Byte)
   frame wr reg : Byte
   post reg = i;

   READ() j : Byte
   frame rd reg : Byte
   post j = reg; 

   ADD(i:Byte)
   frame wr reg : Byte
   pre not oflow(reg, i)
   post reg = reg~ + i;

   SUB(i : Byte)
   frame wr reg : Byte
   pre not uflow(reg,i)
   post reg = reg~ - i
  
   @
     init -> INIT()
 end
