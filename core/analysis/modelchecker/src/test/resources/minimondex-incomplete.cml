values
 N: nat = 10
 V: nat = 10
 M: nat = N*V

types
  Index = nat 
    inv i == i in set {1,...,N}

  Money = nat
    inv m == m in set {0,...,M}
    

channels
  aa
  test,bb : int
  pay, transfer: Index * Index * Money
  accept , reject: Index

    
process Card = 
begin
 state 
 	value: nat := 0
 
 operations
    Init: () ==> ()
    Init() == value := V

    Credit: nat ==> ()
    Credit(n) == value := value + n

    Debit: nat ==> ()
    Debit(n) == value := value - n
       
@ Skip

end
