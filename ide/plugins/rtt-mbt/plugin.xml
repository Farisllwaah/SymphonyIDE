   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.core.internal.resources.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="eu.compassresearch.ide.rttplugin.RttMbtPropertyTester"
            id="eu.compassresearch.ide.rttplugin.RttMbtPropertyTester"
            namespace="eu.compassresearch.ide.rttplugin.RttMbtPropertyTester"
            properties="isRttMbtProject, isTestGenerationContext, isRttTestprocedure"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="RT-Tester Model Based Testing Commands"
            id="eu.compassresearch.ide.rttplugin.RttMbtCommands"
            name="RTT-MBT">
      </category>
      <command
            defaultHandler="eu.compassresearch.ide.rttplugin.RttMbtNewProject"
            id="eu.compassresearch.ide.rttplugin.RttMbtNewProject"
            name="New Project">
      </command>
      <command
            defaultHandler="eu.compassresearch.ide.rttplugin.RttMbtAddProject"
            id="eu.compassresearch.ide.rttplugin.RttMbtAddProject"
            name="Add Project">
      </command>
      <command
            defaultHandler="eu.compassresearch.ide.rttplugin.RttMbtAbort"
            id="eu.compassresearch.ide.rttplugin.RttMbtAbort"
            name="Abort Current Task">
      </command>
      <command
            defaultHandler="eu.compassresearch.ide.rttplugin.RttMbtRetrieveFileOrDirectory"
            id="eu.compassresearch.ide.rttplugin.RttMbtPullFromServer"
            name="Pull from Server">
      </command>
      <command
            defaultHandler="eu.compassresearch.ide.rttplugin.RttMbtSendFileOrDirectory"
            id="eu.compassresearch.ide.rttplugin.RttMbtPushToServer"
            name="Push to Server">
      </command>
      <command
            defaultHandler="eu.compassresearch.ide.rttplugin.RttMbtDeleteFile"
            id="eu.compassresearch.ide.rttplugin.DeleteLocalAndServer"
            name="Delete RTT-MBT Resource">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtGenerateTest"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtGenerateTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtGenerateSimulation"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtGenerateSimulation">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtCleanRttTestProcedure"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtCleanTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtCompileRttTestProcedure"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtCompileTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtRunRttTestProcedure"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtRunTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtReplayTestprocedure"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtReplayTest">
         <activeWhen>
            <or>
               <with
                     variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
                  <equals
                        value="truumpel">
                  </equals>
               </with>
               <with
                     variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
                  <equals
                        value="truumpel">
                  </equals>
               </with>
            </or>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtDocRttTestProcedure"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtDocTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtCheckModel"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtCheckModel">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsModelDumpSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtImportModel"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtImportModel">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsRttMbtProjectSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttplugin.RttMbtCleanProject"
            commandId="eu.compassresearch.ide.rttplugin.RttMbtCleanProject">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsRttMbtProjectSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
   </extension>
