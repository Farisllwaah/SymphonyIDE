#!/bin/bash
JAR=target/commandlinetool-0.0.1-jar-with-dependencies.jar
CLASSPATH=.:../../core/parser/target/parser-0.0.1.jar:../../core/ast/target/ast-0.0.1.jar
if [ ! -f ${JAR} ]; then
echo -e "Executable jar: \n\n${JAR}\n\ndoes not exists, type mvn package to build it."
exit -1;
fi
java -cp ${CLASSPATH} -jar ${JAR} $@